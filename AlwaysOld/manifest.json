
{
    "manifest_version": 2,
    "name": "AlwaysOld",
    "version": "1.0",
  
    "description": "Reloads the reddit page to open as old.reddit.com",
  
    "icons": {
      "16": "icons/favicon-16x16.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://www.reddit.com/*"],
        "js": ["AlwaysOldReddit.js"]
      }
    ]

  }